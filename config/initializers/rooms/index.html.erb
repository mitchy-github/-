<table>
  <thead>
    <tr>
      <th>id</th>
      <th>ルーム画像</th>
      <th>ルーム名</th>
      <th>料金</th>
      <th>ルーム紹介</th>
      <th>作成日</th>
    </tr>
  </thead>
 
  <tbody>
    <% if @rooms.count > 0 %>
      <% @rooms.each do |room| %>
        <tr>
          <td>
            <%= room.id %>
          </td>
          <td>
            <% if room.room_image.attached? %>
              <%= image_tag room.room_image, size: '100' %>
            <% else %>
              <!--<= image_tag asset_path('icon_default_room_image.jpg'), size: '100' %>-->
              画像なし
            <% end %>
          </td>
          <td><%= link_to room.room_name, room %></td>
          <td><%= room.room3_price %></td>
          <td><%= room.room_content %></td>
          <td><%= room.created_at.to_s(:datetime_jp) %></td>
          <!--<td>-->
